{
  "name": "octo",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development.local next dev",
    "dev:test": "env-cmd -f .env.test.local next dev",
    "dev:prod": "env-cmd -f .env.production.local next dev",
    "build:dev": "env-cmd -f .env.development.local next build",
    "build:test": "env-cmd -f .env.test.local next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "yarn check-lint && yarn check-types && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.7",
    "@mui/styled-engine": "^5.2.6",
    "@mui/styled-engine-sc": "latest",
    "@mui/system": "^5.2.6",
    "@next/bundle-analyzer": "^12.1.5",
    "@nivo/core": "0.79.0",
    "@nivo/line": "0.79.0",
    "@types/nprogress": "^0.2.0",
    "axios": "^0.25.0",
    "dayjs": "^1.11.0",
    "env-cmd": "^10.1.0",
    "framer-motion": "^6.2.1",
    "next": "^12.1.4",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^9.0.0",
    "nprogress": "^0.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.34.19",
    "sharp": "^0.30.3",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "8.12.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "husky": "^7.0.4",
    "prettier": "2.5.1",
    "typescript": "4.6.3"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  }
}
