{
  "Typescript React Luffy Component": {
    "prefix": "comp",
    "body": [
      "import React from 'react';",
      "import { styled } from '@mui/material';",
      "",
      "interface ${TM_FILENAME_BASE/(.*)$/${1:/capitalize}/}Props {",
      "\tclassName?: string;",
      "}",
      "const Div = styled('div')<{ active_route?: string }>(",
      "\t({ theme, active_route }) => ({",
      "\t\t'&:hover': {",
      "\t\t\tcolor: `${theme.palette.primary.main} !important`",
      "\t\t},",
      "\t\tcolor: active_route === 'active' ? theme.palette.primary.main : 'inherit',",
      "\t}),",
      ");",
      "const ${TM_FILENAME_BASE/(.*)$/${1:/capitalize}/}: React.FC<${TM_FILENAME_BASE/(.*)$/${1:/capitalize}/}Props> = ({ className }) => {",
      "\treturn <Div className={className} active_route>FK$0</Div>;",
      "};",
      "",
      "export default ${TM_FILENAME_BASE/(.*)$/${1:/capitalize}/};"
    ],
    "description": "Freshket Typescript React Function Component"
  },
